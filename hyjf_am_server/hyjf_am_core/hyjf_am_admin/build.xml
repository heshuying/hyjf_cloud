<project name="hyjf-am-trade" basedir="." default="main">
	
	<target name="copytrade" description="copy auto generator files">
		<delete includeEmptyDirs="true">
			<fileset
				dir="${basedir}/src/main/java/com/hyjf/am/trade/dao/mapper/xml/auto"
				includes="**/*.xml" />
			<fileset
				dir="${basedir}/src/main/java/com/hyjf/am/trade/dao/mapper/auto"
				includes="**/*.java" />
			<fileset
				dir="${basedir}/src/main/java/com/hyjf/am/trade/dao/model/auto"
				includes="**/*.java" />
		</delete>
	
		<copy todir="${basedir}/src/main/java/com/hyjf/am/trade/dao/mapper/xml/auto">
			<fileset dir="../hyjf_am_trade/src/main/java/com/hyjf/am/trade/dao/mapper/xml/auto">
    			<include name="**/*.xml"/>
  			</fileset>
		</copy>
	
		<copy todir="${basedir}/src/main/java/com/hyjf/am/trade/dao/mapper/auto">
			<fileset dir="../hyjf_am_trade/src/main/java/com/hyjf/am/trade/dao/mapper/auto">
    			<include name="**/*.java"/>
  			</fileset>
		</copy>
	
		<copy todir="${basedir}/src/main/java/com/hyjf/am/trade/dao/model/auto">
			<fileset dir="../hyjf_am_trade/src/main/java/com/hyjf/am/trade/dao/model/auto">
    			<include name="**/*.java"/>
  			</fileset>
		</copy>
	</target>
	
